<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="snippet" prefHeight="150.0" prefWidth="600.0" styleClass="sbackground" stylesheets="@../CSS/menus.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.esigelec.snackio.ui.Snippet">
    <Button fx:id="settingsButton" layoutX="505.0" layoutY="81.0" mnemonicParsing="false" text="Settings" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
        <graphic>
            <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../img/setting.png" />
                </image>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="homeButton" layoutX="14.0" layoutY="85.0" mnemonicParsing="false" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0">
        <graphic>
            <ImageView fitHeight="17.0" fitWidth="14.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../img/exit.png" />
                </image></ImageView>
        </graphic>
    </Button>
    <Button fx:id="backButton" layoutX="53.0" layoutY="85.0" mnemonicParsing="false" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="50.0" >
        <graphic>
            <ImageView fitHeight="17.0" fitWidth="14.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../img/back.png" />
                </image></ImageView>
        </graphic>
    </Button>

</AnchorPane>
