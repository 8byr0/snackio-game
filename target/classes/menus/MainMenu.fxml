<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="background" stylesheets="@../CSS/menus.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.esigelec.snackio.ui.MainMenu">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="11.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="openSoloMenuButton" mnemonicParsing="false" onAction="#openSoloMenu" text="SOLO" />
                  <Button fx:id="openMultiMenuButton" mnemonicParsing="false" onAction="#openMultiMenu" text="MULTI" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
